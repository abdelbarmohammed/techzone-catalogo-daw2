{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "nombre",
      "required": true
    },
    "descripcion": {
      "type": "blocks",
      "required": true
    },
    "descripcionCorta": {
      "type": "text",
      "required": true
    },
    "precio": {
      "type": "decimal",
      "required": true
    },
    "imagen": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "imagenes": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "marca": {
      "type": "string",
      "required": true
    },
    "enStock": {
      "type": "boolean",
      "default": true
    },
    "destacado": {
      "type": "boolean",
      "default": false
    },
    "categoria": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categoria.categoria",
      "inversedBy": "productos"
    }
  }
}
